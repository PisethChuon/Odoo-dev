<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!--Point Record on Other Model-->
    <record id="res_partner_packt" model="res.partner">
        <field name="name">Packt publishing</field>
        <field name="city">Birmingham</field>
        <field name="country_id" ref="base.uk"/>
    </record>

    <!--Create a New Record in Current Model-->
    <record id="hostel_room_1" model="hostel.room">
        <field name="name">Hostel Room 01</field>
        <field name="room_no">ROOM-01</field>
        <field name="student_per_room">2</field>
    </record>

</odoo>